# ============================================
# N8N - Configuração da Cloudinha
# ============================================

# Porta do n8n
N8N_PORT=5678

# Credenciais de acesso ao n8n (ALTERAR EM PRODUÇÃO!)
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=changeme_nubo_cloudinha_2024

# URL externa (importante para webhooks)
# Em desenvolvimento: http://localhost:5678
# Em produção: https://seu-dominio.com
N8N_HOST=localhost
N8N_PROTOCOL=http
WEBHOOK_URL=http://localhost:5678

# Timezone
TIMEZONE=America/Sao_Paulo

# ============================================
# PostgreSQL - Banco de dados do n8n
# ============================================

POSTGRES_USER=n8n
POSTGRES_PASSWORD=changeme_postgres_password_2024
POSTGRES_DB=n8n
POSTGRES_NON_ROOT_USER=n8n

# ============================================
# N8N - Configurações avançadas
# ============================================

# Log level: info, debug, warn, error
N8N_LOG_LEVEL=info
N8N_LOG_OUTPUT=console

# ============================================
# Supabase - Integração (preencher depois)
# ============================================

SUPABASE_URL=
SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=

# ============================================
# OpenAI - Integração IA (preencher depois)
# ============================================

OPENAI_API_KEY=

# ============================================
# Anthropic - Integração IA (preencher depois)
# ============================================

ANTHROPIC_API_KEY=
